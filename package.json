{
  "name": "@brontosaurus/web",
  "version": "1.2.0",
  "description": ":boar: Brontosaurus integration for web",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SudoDotDog/Brontosaurus-Web.git"
  },
  "keywords": [
    "brontosaurus"
  ],
  "author": "WMXPY",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SudoDotDog/Brontosaurus-Web/issues"
  },
  "homepage": "https://github.com/SudoDotDog/Brontosaurus-Web#readme",
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "script/**/*.ts",
      "src/**/*.ts"
    ],
    "instrument": true,
    "reporter": [
      "json",
      "text-summary"
    ],
    "report-dir": "coverage",
    "require": [
      "./test/tshook.js"
    ],
    "sourceMap": true
  },
  "devDependencies": {
    "@sudoo/mock": "^1.0.0",
    "@sudoo/triforce": "^1.4.0"
  },
  "dependencies": {
    "@brontosaurus/definition": "^1.3.0"
  }
}